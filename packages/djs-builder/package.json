{
  "name": "@djs-core/builder",
  "version": "1.2.2",
  "description": "Builder for djs-core",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Cleboost",
  "repository": {
    "url": "https://github.com/djs-core/djs-builder"
  },
  "bugs": {
    "url": "https://github.com/djs-core/djs-builder/issues"
  },
  "scripts": {
    "dev": "tsup --config tsup-dev.config.ts",
    "build": "tsup --config tsup-prod.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "npm run build",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "glob": "11.0.1",
    "javascript-obfuscator": "4.1.1",
    "tsup": "8.4.0"
  },
  "engines": {
    "node": ">=18.6.0"
  }
}
