{
  "name": "djs-core",
  "version": "4.2.0",
  "description": "A core library for Discord.js projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Cleboost",
  "repository": {
    "url": "https://github.com/Cleboost/djs-core",
    "directory": "packages/djs-core"
  },
  "bugs": {
    "url": "https://github.com/Cleboost/djs-core/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --minify --format cjs --out-dir dist",
    "watch": "tsup src/index.ts --dts --format cjs --out-dir dist --watch src/**/*",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "discord.js": "^14.18.0"
  },
  "engines": {
    "node": ">=18.6.0"
  },
  "packageManager": "pnpm@10.10.0"
}
