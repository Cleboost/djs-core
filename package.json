{
  "name": "djs-core",
  "version": "1.1.2",
  "description": "A core library for Discord.js projects",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Cleboost",
  "bin": {
    "djs-core": "index.js"
  },
  "repository": {
    "url": "https://github.com/Cleboost/djs-core"
  },
  "bugs": {
    "url": "https://github/com/Cleboost/djs-core/issues"
  },
  "scripts": {
    "dev": "tsup --config tsup-dev.config.ts",
    "build": "tsup --config tsup-prod.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "npm run build && jest",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "chalk": "5.4.1",
    "chokidar": "4.0.3",
    "commander": "13.1.0",
    "discord.js": "14.17.3",
    "kolorist": "1.8.0",
    "ora": "5.4.1",
    "tsup": "8.3.6"
  },
  "devDependencies": {
    "@eslint/js": "9.18.0",
    "@types/node": "22.10.10",
    "eslint": "9.18.0",
    "eslint-plugin-headers": "1.2.1",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3",
    "typescript-eslint": "8.21.0"
  },
  "engines": {
    "node": ">=16.6.0"
  }
}
